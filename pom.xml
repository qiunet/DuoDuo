	<modelVersion>4.0.0</modelVersion>

	<groupId>io.github.qiunet</groupId>
	<artifactId>DuoDuo</artifactId>
	<version>7.0.9</version>
	<name>DuoDuo</name>

	<modules>
		<module>QiunetUtils</module>
		<module>QiunetDatas</module>
